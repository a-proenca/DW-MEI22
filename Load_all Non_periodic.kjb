    <entry>
      <name>Create Materialized Views for Vendas</name>
      <description/>
      <type>SQL</type>
      <attributes/>
      <sql>CREATE MATERIALIZED VIEW Montante_Vendas_ano AS
SELECT date_trunc('month', "g04dw"."facts_vendas"."data") AS "data", sum("g04dw"."facts_vendas"."montante") AS "sum"
FROM "g04dw"."facts_vendas"
GROUP BY date_trunc('month', "g04dw"."facts_vendas"."data")
ORDER BY date_trunc('month', "g04dw"."facts_vendas"."data") ASC;


CREATE INDEX data_indx_view ON g04dw.facts_vendas (data);
CREATE INDEX montante_indx_view ON g04dw.facts_vendas (montante);

CREATE MATERIALIZED VIEW Montante_Vendas_agencia_ano AS
SELECT date_trunc('year', "g04dw"."facts_vendas"."data") AS "data", "Dim Imobiliaria"."nome" AS "Dim Imobiliaria__nome", sum("g04dw"."facts_vendas"."montante") AS "sum"
FROM "g04dw"."facts_vendas"
LEFT JOIN "g04dw"."dim_imobiliaria" "Dim Imobiliaria" ON "g04dw"."facts_vendas"."idimobiliaria" = "Dim Imobiliaria"."id_imobiliaria"
GROUP BY date_trunc('year', "g04dw"."facts_vendas"."data"), "Dim Imobiliaria"."nome"
ORDER BY date_trunc('year', "g04dw"."facts_vendas"."data") ASC, "Dim Imobiliaria"."nome" ASC;

CREATE INDEX indx_idimobiliaria_view ON g04dw.facts_vendas (idimobiliaria);
CREATE INDEX indx_id_imobiliaria_view ON g04dw.dim_imobiliaria (id_imobiliaria);
CREATE INDEX indx_nome_view ON g04dw.dim_imobiliaria (nome);</sql>
